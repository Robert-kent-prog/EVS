{
  "expo": {
    "name": "EVS",
    "slug": "EVS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/EVS.png",
    "scheme": "evs",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow EVS to save PDF files to your photo library.",
        "NSPhotoLibraryAddUsageDescription": "Allow EVS to save PDF files to your photo library.",
        "LSSupportsOpeningDocumentsInPlace": true,
        "UISupportsDocumentBrowser": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/EVS.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.DOWNLOAD_WITHOUT_NOTIFICATION"
      ],
      "package": "com.user_name123.EVS"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow EVS to access your photos.",
          "cameraPermission": "Allow EVS to access your camera."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow EVS to access your photos.",
          "savePhotosPermission": "Allow EVS to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-secure-store",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0886769a-9fb1-4569-a15a-b2bb157653f9"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0886769a-9fb1-4569-a15a-b2bb157653f9"
    }
  }
}
